#include <stdio.h>

int toplam(int n){
    
    int sum=0;
    
    for(int i=1;i<=n;i++){
        if(i % 3 !=0 && i % 5 !=0){
            sum += i;
        }
    }
    return sum;
}

int main(){
    
    int n;
    
    printf(" n = ");
    scanf("%d",&n);
    
    printf("sum = %d ",toplam(n));
    
}
