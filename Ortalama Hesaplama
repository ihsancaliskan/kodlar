#include <stdio.h>

double ortalama(double x , double y , double z){
double result=0.0;


return result = (x+y+z) / 3.0;
  
}

int main() {

double x,y,z;

printf("x = ");
scanf("%lf",&x);
printf("y = ");
scanf("%lf",&y);
printf("z = ");
scanf("%lf",&z);

printf(" Ortalama = %lf",ortalama(x,y,z));

}
