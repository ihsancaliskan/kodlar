#include <stdio.h>

float bolme(int k , int x){
    
    if(x == 0){
        printf("Hata bir sayı 0 a bölünemez!!");
        return 0.0;
    }
        return k / x;
}

int main(){
    
    int k,x;
    
    
    printf("k = ");
    scanf("%d",&k);
    printf("x = ");
    scanf("%d",&x);
    
    printf("sonuç = %f",bolme(k,x));
    
    return 0;
}
